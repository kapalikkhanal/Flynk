{
  "expo": {
    "name": "The Headlines",
    "slug": "flynk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flynk_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/flynk_splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flynk.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kapalikkhanal.flynk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/flynk.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b9374c3-c222-4a5b-a2b6-5c80601a0d03"
      }
    }
  }
}
